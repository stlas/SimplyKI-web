{
  "name": "simplyKI-web",
  "version": "1.0.0",
  "description": "SimplyKI Web Frontend - Meta-Framework for AI Development Tools",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview --host 0.0.0.0",
    "serve": "node server.js",
    "serve:prod": "NODE_ENV=production node server.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "test:unit": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "lucide-vue-next": "^0.294.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.18.1",
    "lighthouse": "^11.3.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.11.0",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "AI",
    "development",
    "tools",
    "meta-framework",
    "SimplyKI",
    "vue",
    "vite"
  ],
  "author": "sTLAs <stlas@simplyki.net>",
  "license": "MIT",
  "homepage": "https://simplyki.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/stlas/SimplyKI.git"
  },
  "bugs": {
    "url": "https://github.com/stlas/SimplyKI/issues"
  }
}
